<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pSQL">

	<select id="getPark" resultType="mVO" parameterType="mVO">
		SELECT
			pno, pname, pmis, pcity, pkreview
		FROM
			park
		WHERE
			pno = #{pno}
	</select>

</mapper>